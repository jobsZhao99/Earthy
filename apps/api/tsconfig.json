{
    "compilerOptions": {
      "target": "ES2020",
      "lib": ["ES2020"],
      "module": "NodeNext",
      "moduleResolution": "NodeNext",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "outDir": "dist",
      "resolveJsonModule": true,
      "types": ["node"]
    },
    "include": ["src/**/*"]
  }
  
{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["ES2020"],
    "module": "NodeNext",
    "moduleResolution": "NodeNext",

    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,

    "outDir": "dist",
    "types": ["node"]
  },
  "include": ["src/**/*", "scripts/**/*"],
  "exclude": ["scr/routes/Temp/**/*","scr/routes/Archieved/**/*"]
  
}